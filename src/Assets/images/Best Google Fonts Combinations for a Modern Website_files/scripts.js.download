	// Run Slack Invite on Request
	jQuery('.slack-invite').click(function(){
		
		// grab handler
		var slackInvite = jQuery(this);

		// set cursor to waiting
		jQuery('html,body,.slack-invite').css('cursor', 'wait');

		// bring token
		jQuery.post(ajaxurl, {
			action: 'at_get_slack_token',
		}, function (response) {
			// then run the invite
			jQuery.ajax({
				type: 'POST',
				url: 'https://slack.com/api/users.admin.invite',
				data: {'token':response, 'email':slackInvite.data('email'), 'first_name':slackInvite.data('fname'), 'last_name':slackInvite.data('lname')},
				contentType: 'application/x-www-form-urlencoded',
				dataType: 'json',
				success: function(data){ 
					// set cursor back to normal
					jQuery('html,body,.slack-invite').css('cursor', 'default');
					// Save the response for this user if 'ok' and give feedback
					if ( data['ok'] == true ) {
						jQuery.post(ajaxurl, { action: 'at_save_slack_invite' });
						jQuery('.slack-invite').fadeOut('100');
						jQuery('.slack-invite-response').delay('600').fadeIn('200');
					}
				}
			});
		});

		return false;
	
	});


